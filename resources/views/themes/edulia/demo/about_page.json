                    "speech_user_image": {
                        "value": [
                            "{\"type\":\"image\",\"name\":\"2.jpg\",\"path\":\"optionbuilder\\\/uploads\\\/447611-10-2023_1250pm1.jpg\",\"mime\":\"jpg\",\"size\":41661,\"thumbnail\":\"[App_url]/public\\\/storage\\\/optionbuilder\\\/uploads\\\/447611-10-2023_1250pm1.jpg\"}"
