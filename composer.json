{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
      "framework",
      "laravel"
  ],
  "license": "MIT",
  "require": {
      "php": "^8.2",
      "africastalking/africastalking": "v3.0.0",
      "anhskohbo/no-captcha": "^3.2",
      "barryvdh/laravel-dompdf": "^3.1.1",
      "brian2694/laravel-toastr": "^5.53",
      "fakerphp/faker": "^1.24",
      "google/apiclient": "^2.12",
      "guzzlehttp/guzzle": "^6.0|^7.2.0",
      "intervention/image": "^2.5",
      "ixudra/curl": "^6.22",
      "jenssegers/agent": "^2.6",
      "joisarjignesh/bigbluebutton": "dev-master",
      "laravel/framework": "^12.0",
      "laravel/passport": "^12.0",
      "laravel/pulse": "^1.4",
      "laravel/sanctum": "^4.0",
      "laravel/tinker": "^2.8.1",
      "laravelcollective/html": "^6.1",
      "league/omnipay": "^3.2",
      "maatwebsite/excel": "^3.1",
      "mercadopago/dx-php": "^2.4",
      "nwidart/laravel-modules": "^8.2",
      "omnipay/paypal": "^3.0",
      "php-http/message": "^1.16",
      "php-http/message-factory": "^1.1",
      "phpmailer/phpmailer": "^6.0",
      "pusher/pusher-php-server": "^5.0",
      "rahulreghunath/textlocal": "^1.0",
      "silviolleite/laravelpwa": "^2.0",
      "simplesoftwareio/simple-qrcode": "~4",
      "spatie/laravel-html": "^3.11",
      "spatie/valuestore": "^1.2",
      "spondonit/laravel-fcm-notification": "^1.0.1",
      "spondonit/rpclient": "^1.0",
      "spondonit/school-service": "^1.2",
      "stripe/stripe-php": "^7.37",
      "symfony/polyfill-php70": "^1.18.1",
      "tarsoft/toyyibpay": "^v1.0.0",
      "twilio/sdk": "^5.31",
      "unicodeveloper/laravel-paystack": "dev-l12-compatibility",
      "vimeo/laravel": "dev-l12-compatibility",
      "xendit/xendit-php": "^2.10",
      "yajra/laravel-datatables-oracle": "^12.0"
  },
  "require-dev": {
      "barryvdh/laravel-debugbar": "^3.8",
      "larastan/larastan": "^3.1",
      "laravel/dusk": "^8.0",
      "laravel/pint": "^1.21",
      "laravel/sail": "^1.14",
      "laravel/ui": "^4.6.1",
      "mockery/mockery": "^1.3.1",
      "nunomaduro/collision": "^8.1",
      "phpunit/phpunit": "^11.0",
      "rector/rector": "^2.0",
      "spatie/laravel-ignition": "^2.0"
  },
  "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true,
      "allow-plugins": {
          "php-http/discovery": true
      }
  },
  "extra": {
      "laravel": {
          "dont-discover": [
              "laravel/telescope"
          ]

      },
      "google/apiclient-services": [
          "Calendar","Oauth2"
      ]
  },
  "autoload": {
      "classmap": [
          "database"
      ],
      "psr-4": {
          "App\\": "app/",
          "Tests\\": "tests/",
          "Modules\\": "Modules/",
          "Database\\Factories\\": "database/factories/",
          "Database\\Seeders\\": "database/seeders/",
          "Larabuild\\Pagebuilder\\": "packages/larabuild/pagebuilder/src/",
          "Larabuild\\Optionbuilder\\": "packages/larabuild/optionbuilder/src/"
      },
      "files": [
          "app/Helpers/Helper.php",
          "app/Helpers/saas.php",
          "app/Helpers/FeesHelper.php",
          "app/Helpers/ExamHelper.php",
          "app/Helpers/Basic.php"
      ]
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"

      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
      "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
      "post-autoload-dump": [
          "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
          "@php artisan package:discover --ansi"
      ],
      "post-root-package-install": [
          "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ],
      "post-create-project-cmd": [
          "@php artisan key:generate --ansi"
      ],
      "lint": "pint",
      "refactor": "rector",
      "test:lint": "pint --test",
      "test:refactor": "rector --dry-run",
      "test": [
          "@test:lint",
          "@test:refactor"
      ]
  },
  "repositories": {
      "optionbuilder": {
          "type": "path",
          "url": "packages/larabuild/optionbuilder",
          "options": {
              "symlink": false
          }
      },
      "pageubilder": {
          "type": "path",
          "url": "packages/larabuild/pagebuilder",
          "options": {
              "symlink": false
          }
      },
      "shift-vimeo" : {
          "type": "vcs",
          "url": "https://github.com/laravel-shift/laravel.git"
      },
      "shift-paystack" : {
          "type": "vcs",
          "url": "https://github.com/laravel-shift/laravel-paystack.git"
      },
      "aora-toyyibpay" : {
          "type": "vcs",
          "url": "https://github.com/aoradev44/toyyibpay-laravel.git"
      },
      "aora-html" : {
          "type": "vcs",
          "url": "https://github.com/aoradev44/html.git"
      }

  }
}

